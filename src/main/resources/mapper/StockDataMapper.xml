<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.springCloud.mapper.StockDataMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.example.springCloud.entity.StockData">
        <id column="id" property="id" />
        <result column="code_value" property="codeValue" />
        <result column="code_name" property="codeName" />
        <result column="price_limit_value" property="priceLimitValue" />
        <result column="price_limit_status" property="priceLimitStatus" />
        <result column="now_price" property="nowPrice" />
        <result column="open_price" property="openPrice" />
        <result column="limit_up" property="limitUp" />
        <result column="create_time" property="createTime" />
        <result column="create_day" property="createDay" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, code_value, code_name, price_limit_value, price_limit_status, now_price, open_price, limit_up, create_time, create_day
    </sql>

</mapper>
